module Kivu12 {
   board kivu12
   material aluminum
   header { label "Kivu12" }

   control freq_pot Pot {
      position 7.33hp, 111mm
      style knurled
      label "FREQ"
      pin P1
   }

   control freq2_pot Pot {
      position 7.33hp, 111mm
      style knurled
      label "FREQ"
      pin P2
   }

   control audio_out1 AudioOut {
      position 7.33hp, 111mm
      style knurled
      label "OUT L"
      pin AO1
   }

   control audio_out2 AudioOut {
      position 10hp, 111mm
      style knurled
      label "OUT R"
      pin AO2
   }
}
