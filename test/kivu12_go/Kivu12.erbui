module Kivu12 {
   board kivu12
   material aluminum
   header { label "Kivu12" }

   control go1 GateOut {
      position 7.33hp, 111mm
      style knurled
      label "GO1"
      pin GO1
   }

   control go2 GateOut {
      position 7.33hp, 111mm
      style knurled
      label "GO2"
      pin GO2
   }

   control audio_out1 AudioOut {
      position 7.33hp, 111mm
      style knurled
      label "OUT L"
      pin AO1
   }

   control audio_out2 AudioOut {
      position 10hp, 111mm
      style knurled
      label "OUT R"
      pin AO2
   }
}
