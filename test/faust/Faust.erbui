// Faust.erbui

module Faust {
   width 12hp
   board kivu12
   material aluminum
   header { label "Faust" }

   control freq Pot {
      position 6hp, 34mm
      style rogan, 6ps
      label "FREQ"
   }

   control audio_in AudioIn {
      position 6hp, 96mm
      style knurled
      label "IN"
   }

   control audio_out AudioOut {
      position 6hp, 111mm
      style knurled
      label "OUT"
   }
}
